{
  "name": "grey-jcr",
  "version": "2.0.0",
  "description": "Grey College JCR Website",
  "homepage": "https://greyjcr.com",
  "licence": "MIT",
  "private": true,
  "scripts": {
    "build-ui": "webpack --config src/ui/webpack.config.js && cp -r src/public/* dist/ui/",
    "build-api": "tsc && cp -r src/views dist/",
    "build": "rm -rf dist && npm run build-ui && npm run build-api",
    "start": "node ./bin/www",
    "test": "mocha dist/test --reporter spec --recursive --exit",
    "migrate-postgres": "knex migrate:latest --knexfile dist/knexfile.js",
    "seed-postgres": "knex seed:run --knexfile dist/knexfile.js",
    "storybook": "start-storybook -p 9001 -c src/ui/.storybook"
  },

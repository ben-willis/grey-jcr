  "description": "Grey College JCR Website",
  "homepage": "https://greyjcr.com",
  "licence": "MIT",
  "private": true,
  "scripts": {
    "build-ui": "webpack --config src/ui/webpack.config.js",
    "build-api": "tsc && cp -r src/static src/views dist/ && cp -r src/public/* dist/ui/ && cp -r src/users/tests/files dist/users/tests",
    "build": "rm -rf dist && npm run build-ui && npm run build-api",
    "start": "node ./bin/www",
    "test": "mocha dist/test dist/**/*.spec.js --reporter spec --recursive --exit",
    "migrate": "typeorm migration:run -c grey",
    "seed": "node ./bin/manage-fixtures load",
    "storybook": "start-storybook -p 9001 -c src/ui/.storybook"
  },
